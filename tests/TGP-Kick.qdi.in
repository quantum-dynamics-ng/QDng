<qdng>
<propa dt="20" steps="1000" wcycle="1000" dir="TGP-Kick" spectrum="spectrum.dat">
	<propagator name="Cheby">
	  <hamiltonian name="Sum">
	      <A name="GridNablaSq" dims="1" mass0="1763.2" key="T"/>
	      <B name="GridPotential" file="@srcdir@/GridPot0" key="V"/>
	  </hamiltonian>
	</propagator>
        <wf file="@srcdir@/GridPot0_ef_0" normalize="true"/>
        <filterinit>
             <apply name="GridKick" dims="1" k0="1"/>
        </filterinit>
        <filterpost>
            <expeconly name="GridMomentum"/>
            <expeconly name="GridPosition"/>
            <expeconly name="Projection" files="@srcdir@/GridPot0_ef" num="4"/>
            <expeconly ref="T"/>
            <expeconly ref="V"/>
        </filterpost>
</propa>
</qdng>
