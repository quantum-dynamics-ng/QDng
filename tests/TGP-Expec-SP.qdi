<qdng>
<propa dt="20" steps="1000" wcycle="1000" dir="TGP-Expec-SP" spectrum="spectrum.dat">
	<propagator name="Cheby">
	  <hamiltonian name="Sum">
	      <A name="GridNablaSq" dims="1" mass0="1763.2" key="T"/>
	      <B name="GridPotential" file="GridPot0" key="V"/>
	  </hamiltonian>
	</propagator>
        <wf name="LC" normalize="true">
            <wf0 file="GridPot0_ef_0"/>
            <wf1 file="GridPot0_ef_1"/>
            <wf2 file="GridPot0_ef_2"/>
            <wf3 file="GridPot0_ef_3"/>
        </wf>
        <filterpost>
            <expeconly name="GridMomentum"/>
            <expeconly name="GridPosition"/>
            <expeconly name="Projection" files="GridPot0_ef" num="4"/>
            <expeconly ref="T"/>
            <expeconly ref="V"/>
        </filterpost>
</propa>
</qdng>
