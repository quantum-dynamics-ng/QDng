<qdng>
<propa dt="20" steps="1000" wcycle="1000" dir="TGP-Expec" spectrum="spectrum.dat">
	<propagator name="Cheby">
	  <hamiltonian name="Sum">
	      <A name="GridNablaSq" dims="1" mass0="1763.2" key="T"/>
	      <B name="GridPotential" file="GridPot0" key="V"/>
	  </hamiltonian>
	</propagator>
        <wf file="GridPot0_ef_0" normalize="true"/>
        <filterpost>
            <expeconly name="GridMomentum"/>
            <expeconly name="GridPosition"/>
            <expeconly name="Projection" files="GridPot0_ef" num="4"/>
            <expeconly ref="T"/>
            <expeconly ref="V"/>
        </filterpost>
</propa>
</qdng>
