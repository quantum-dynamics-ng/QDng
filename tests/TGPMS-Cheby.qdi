<qdng>
<propa dt="10" steps="1000" wcycle="1000" dir="TGPMS-Cheby" spectrum="spectrum.dat">
	<propagator name="Cheby">
	  <hamiltonian name="Multistate">
            <m0.0 name="Sum">
               <Tkin name="GridNablaSq" dims="1" mass0="1763.2" key="T"/>
               <Vpot name="GridPotential" file="GridPot0" key="V0"/>
            </m0.0>
            <m1.1 name="Sum">
               <Tkin ref="T"/>
               <Vpot name="GridPotential" file="GridPot1" key="V1"/>
            </m1.1>
        </hamiltonian>
	</propagator>
        <wf name="Multistate" normalize="true">
         <wf0 file="GridPot0_ef_0"/>
         <wf1 file="GridPot0_ef_0"/>
        </wf>
        <filterpost>
            <expeconly name="Multistate" unity="false" states="2">
               <m0.0 name="GridMomentum" key="p"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m1.1 ref="p"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m0.0 name="GridPosition" key="x"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m1.1 ref="x"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m0.0 ref="T"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m1.1 ref="T"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m0.0 ref="V0"/>
            </expeconly>
            <expeconly name="Multistate" unity="false" states="2">
               <m1.1 ref="V1"/>
            </expeconly>
        </filterpost>
</propa>
</qdng>
