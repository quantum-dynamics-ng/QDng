@article{citeulike:10460406,
    abstract = {{A propagation method for the time dependent Schr\"odinger equation was
studied leading to a general scheme of solving ode type equations. Standard
space discretization of time-dependent pde's usually results in system of ode's
of the form u\_t -Gu = s where G is a operator (matrix) and u is a
time-dependent solution vector. Highly accurate methods, based on polynomial
approximation of a modified exponential evolution operator, had been developed
already for this type of problems where G is a linear, time independent matrix
and s is a constant vector. In this paper we will describe a new algorithm for
the more general case where s is a time-dependent r.h.s vector. An iterative
version of the new algorithm can be applied to the general case where G depends
on t or u. Numerical results for Schr\"odinger equation with time-dependent
potential and to non-linear Schr\"odinger equation will be presented.}},
    author = {Tal-Ezer, Hillel and Kosloff, Ronnie and Schaefer, Ido},
    day = {17},
    doi = {10.1007/s10915-012-9583-x},
    eprint = {1204.3766},
    issn = {0885-7474},
    journal = {Journal of Scientific Computing},
    keywords = {numerics, qd, theory},
    month = apr,
    number = {1},
    pages = {211--221},
    posted-at = {2012-12-11 13:10:58},
    priority = {2},
    publisher = {Springer Netherlands},
    title = {{New, Highly Accurate Propagator for the Linear and Nonlinear Schr\"odinger Equation}},
    url = {http://dx.doi.org/10.1007/s10915-012-9583-x},
    volume = {53},
    year = {2012}
}

@article{citeulike:2199392,
    abstract = {{High-harmonic generation provides an attractive light source of coherent radiation in the extreme-ultraviolet (XUV) and soft-x-ray regions of the spectrum and allows for the production of single attosecond pulses or pulse trains. This Colloquium covers the control of high-harmonic spectra by temporal and spatial pulse shaping of the driving laser pulses and its implications on time-resolved XUV spectroscopy and attosecond pulse shaping. It summarizes important steps for extending existing pulse shaping techniques and control schemes from the near-infrared or visible part to shorter wavelengths. Using adaptive pulse shaping of the driving laser pulses, several groups have demonstrated control of the high-harmonic spectrum, including the author's work on the complete control over the XUV spectrum of high-order harmonics, generated in a gas-filled hollow fiber. It is possible to achieve both the enhancement and the suppression of single or several selected harmonic orders. These arbitrarily shaped soft-x-ray spectra will allow for important modifications of the resulting harmonic pulses in the temporal domain. This constitutes first steps towards direct attosecond pulse shaping in the soft-x-ray domain. Moreover, high-harmonic generation in a hollow-core fiber can be enhanced by coupling into a single fiber mode using a feedback-controlled adaptive two-dimensional spatial light modulator.}},
    author = {Winterfeldt, Carsten and Spielmann, Christian and Gerber, Gustav},
    doi = {10.1103/RevModPhys.80.117},
    journal = {Reviews of Modern Physics},
    month = jan,
    number = {1},
    pages = {117--140},
    posted-at = {2012-12-11 09:57:48},
    priority = {2},
    publisher = {American Physical Society},
    title = {{Colloquium: Optimal control of high-harmonic generation}},
    url = {http://dx.doi.org/10.1103/RevModPhys.80.117},
    volume = {80},
    year = {2008}
}

@misc{citeulike:11838552,
    abstract = {{Coherent control of harmonic generation was studied theoretically. An optimal
control theory was employed to find the driving field where restrictions were
put on the frequency band. Additional restrictions were added to suppress
undesired outcomes such as ionization and dissociation. The method was
formulated in the frequency domain where a relaxation updated procedure was
employed. The method was tested on several examples demonstrating the ability
to generate higher frequencies than the restricted frequency band of a driving
field.}},
