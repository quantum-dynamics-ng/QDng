    citeulike-linkout-2 = {http://link.aps.org/pdf/PRA/v78/i3/e033814},
    doi = {10.1103/PhysRevA.78.033814},
    journal = {Physical Review A},
    keywords = {hhg, theory},
    month = sep,
    pages = {033814+},
    posted-at = {2012-08-29 11:47:31},
    priority = {2},
    publisher = {American Physical Society},
    title = {{Harmonic generation: Quantum-electrodynamical theory of the harmonic photon-number spectrum}},
    url = {http://dx.doi.org/10.1103/PhysRevA.78.033814},
    volume = {78},
    year = {2008}
}

