@article{citeulike:1622057,
    abstract = {{A new computational method for determining the eigenvalues and eigenfunctions of the Schr\"{o}dinger equation is described. Conventional methods for solving this problem rely on diagonalization of a Hamiltonian matrix or iterative numerical solutions of a time independent wave equation. The new method, in contrast, is based on the spectral properties of solutions to the time-dependent Schrodinger equation. The method requires the computation of a correlation function \~{a}\"{I}(r, 0)| \"{I}(r, t)\~{a} from a numerical solution \"{I}(r, t). Fourier analysis of this correlation function reveals a set of resonant peaks that correspond to the stationary states of the system. Analysis of the location of these peaks reveals the eigenvalues with high accuracy. Additional Fourier transforms of \"{I}(r, t) with respect to time generate the eigenfunctions. The effectiveness of the method is demonstrated for a one-dimensional asymmetric double well potential and for the two-dimensional H\'{e}non-Heiles potential.}},
    author = {Feit, M. D. and Fleck, J. A. and Steiger, A.},
    citeulike-article-id = {1622057},
